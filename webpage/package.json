{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "@mrhut10",
  "dependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "autoprefixer": "^9.7.4",
    "dotenv": "^8.1.0",
    "gatsby": "^2.19.41",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-google-analytics": "^2.1.37",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-manifest": "^2.2.47",
    "gatsby-plugin-offline": "^2.2.9",
    "gatsby-plugin-postcss": "^2.1.21",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.12",
    "gatsby-plugin-sitemap": "^2.2.29",
    "gatsby-plugin-typescript": "^2.2.2",
    "gatsby-source-filesystem": "^2.1.54",
    "gatsby-source-sanity": "^5.0.6",
    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.3.18",
    "libreact": "^2.13.1",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.1",
    "ramda": "^0.26.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-lazyload": "^2.6.2",
    "slugify": "^1.4.0",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gatsby-cli": "^2.10.9",
    "prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-ts-node": "tsc -p ts_node/tsconfig.json",
    "dev-ts-node": "tsc -p ts_node/tsconfig.json -w",
    "build": "npm run build-ts-node && node ./node_modules/gatsby-cli/lib/index.js build",
    "dev": "npm run dev-ts-node && node ./node_modules/gatsby-cli/lib/index.js develop --open",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run dev",
    "serve": "node ./node_modules/gatsby-cli/lib/index.js serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "netlify deploy --dir=public",
    "deploy-live": "netlify deploy --dir=public --prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
